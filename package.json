{
  "name": "ai-logo-generator-platform",
  "version": "0.1.0",
  "description": "AI-powered web app that creates professional logos based on user requirements and industry-specific templates",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run client\"",
    "install:client": "cd client && npm install",
    "build:client": "cd client && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxaginfo/ai-logo-generator-platform.git"
  },
  "keywords": [
    "ai",
    "logo",
    "generator",
    "design",
    "branding",
    "startup",
    "business"
  ],
  "author": "AI Web App Generator",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dxaginfo/ai-logo-generator-platform/issues"
  },
  "homepage": "https://github.com/dxaginfo/ai-logo-generator-platform#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.8.0",
    "redis": "^4.6.7",
    "stripe": "^13.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}